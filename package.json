{
	"name": "sort-strings-to-json",
	"displayName": "Sort-Strings-To-Json",
	"publisher": "zeroMaster",
	"description": "zeroMaster's second extension project",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.sortString"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.sortString",
				"title": "Sort String"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Strings Sort to Json",
			"properties": {
				"sort-string-2-json.copyFromClipboard": {
					"type": "boolean",
					"default": true,
					"description": "Using clipboard's value",
					"scope": "window"
				},
				"sort-string-2-json.destPath": {
					"type": "string",
					"default": "..",
					"description": "Set json file path",
					"scope": "window"
				},
				"sort-string-2-json.destFileName": {
					"type": "string",
					"default": "Strings.ko.json",
					"description": "Set json file name",
					"scope": "window"
				}
			}
		}
	},
	"scripts": {
		"main": "node ./main/runTest.js"
	},
	"repository": {
		"url": "https://github.com/SuperLeeK/sort_string_to_json"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"fs": "0.0.1-security"
	}
}
